    "react-router-dom": "^4.2.2",
    "sqlite": "^2.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bable-loader": "^0.0.1-security",
    "enzyme": "^3.3.0",
    "jest": "^22.2.2",
    "nock": "^9.1.6",
    "nodemon": "^1.14.12",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
